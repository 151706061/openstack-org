{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "3.1.12",
		"silverstripe/framework": "3.1.12",
        "silverstripe/translatable": "2.0.6",
        "ezyang/htmlpurifier": "dev-master",
        "undefinedoffset/sortablegridfield":"dev-master",
        "tractorcow/silverstripe-colorpicker":"3.0.*@dev",
        "knplabs/github-api": "1.4.4",
        "rackspace/php-opencloud": "1.12.2",
        "nnnick/chartjs": "1.0.1.*@dev",
        "openid/php-openid": "dev-master",
        "silverstripe/geoip": "dev-master"
	},
   "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "classmap": [
            "geoip/code",
            "html2pdf_v4.03"
        ]
    },
	"config": {
		"process-timeout": 600	
	},
	"minimum-stability": "dev"
}
