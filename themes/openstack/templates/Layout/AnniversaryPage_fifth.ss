<header class="header">
    <div class="container">
        <div class="row-fluid">
            <div class="span10 offset1">
                <div class="five">
                    <img class="five-arm" src="{$ImgPath}/top-arm-rotated.png">
                    <img class="five-number" alt="Happy anniversary!" class="header-title" src="{$ImgPath}/happy-anniversary.png">
                </div>
                <h1>OpenStack - 5th anniversary</h1>

                <div class="flying-robot">
                    <img src="{$ImgPath}/flying-robot.png">
                </div>

                <div class="openstack">
                    <img class="openstack-bottom" src="{$ImgPath}/openstack.png">
                    <img class="openstack-arms" src="{$ImgPath}/openstack-arms.png">
                </div>
            </div>
            <img alt="" class="header-robot" src="{$ImgPath}/bot-1.png">
        </div>
    </div>
</header><!-- </header>  -->

<section class="statics">
    <div class="container">
        <div class="row-fluid">
            <div class="span4 members">
                <div class="icon">
                    <img alt="" src="{$ImgPath}/icon-members.png">
                </div>
                <div class="statics-txt">
                    <p>{$MembersCount}</p>
                    <p class="misce">Openstack Members</p>
                </div>
            </div>
            <div class="span4 countries">
                <div class="icon">
                    <img alt="" src="{$ImgPath}/icon-countries.png">
                </div>
                <div class="statics-txt">
                    <p>{$CountryCount}</p>
                    <p class="misce">Countries with Registered Community Members</p>
                </div>
            </div>
            <div class="span4 organizations">
                <div class="icon">
                    <img alt="" src="{$ImgPath}/icon-organizations.png">
                </div>
                <div class="statics-txt">
                    <p>{$OrganizationCount}</p>
                    <p class="misce">Organizations involved</p>
                </div>
            </div>
        </div>
    </div>
</section> <!-- </statics>  -->

<section class="map">
